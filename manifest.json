{
    "name": "Stackathon",
    "description" : "Amazon Redirecting Extension",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["*://*.amazon.com/*"],
            "js": ["content.js"]
        }
    ],
      "background": {
          "scripts": ["background.js"]
      },
      "browser_action": {
          "default_icon": "hello_extensions.png"
      }
  }